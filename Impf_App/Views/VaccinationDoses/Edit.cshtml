@model Impf_App.Models.VaccinationDosis

@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Edit</h1>

<h4>VaccinationDosis</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="P_Dosis_Id" />
            <div class="form-group">
                <label asp-for="P_Dosis_Id" class="control-label"></label>
                <div>@Html.DisplayFor(model => model.P_Dosis_Id)</div>
            </div>
            <div class="form-group">
                <label asp-for="F_Vaccine" class="control-label"></label>
                <select asp-for="F_VaccineP_VaccineId" class="form-control" asp-items="ViewBag.F_Vaccine">
                    <option value="">-- Select Vaccine --</option>
                </select>
                <span asp-validation-for="F_VaccineP_VaccineId" class="text-danger" />
            </div>
            <div class="form-group">
                <label asp-for="F_Patient" class="control-label"></label>
                <select asp-for="F_PatientP_InsuranceNr" class="form-control" asp-items="ViewBag.F_Patient">
                    <option value="">-- Select Patient --</option>
                </select>
                <span asp-validation-for="F_PatientP_InsuranceNr" class="text-danger" />
            </div>
            <div class="form-group">
                <label asp-for="ProductionDate" class="control-label"></label>
                <input asp-for="ProductionDate" class="form-control" />
                <span asp-validation-for="ProductionDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="VaccinationDate" class="control-label"></label>
                <input asp-for="VaccinationDate" class="form-control" />
                <span asp-validation-for="VaccinationDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Place" class="control-label"></label>
                <input asp-for="Place" class="form-control" />
                <span asp-validation-for="Place" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Doctor" class="control-label"></label>
                <input asp-for="Doctor" class="form-control" />
                <span asp-validation-for="Doctor" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

