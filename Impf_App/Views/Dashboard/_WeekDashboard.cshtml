@model Impf_App.Models.Dashboard

@{
    ViewData["Title"] = "Weekly Dashboard";
}

<h2>@ViewData["Title"]</h2>

<table class="table">
    <thead>
        <tr>
            <th>
                Vaccine
            </th>
            <th>
                Patient
            </th>
            <th>
                Production Date
            </th>
            <th>
                Vaccination Date
            </th>
            <th>
                Place
            </th>
            <th>
                Doctor
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.WeekList) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.F_Vaccine.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.F_Patient.FullName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProductionDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VaccinationDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Place)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Doctor)
            </td>
        </tr>
}
    </tbody>
</table>
